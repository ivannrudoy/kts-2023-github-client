  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch localhost",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "url": "http://localhost:9002",
      "webRoot": "${workspaceFolder}",
      "pathMappings": [
        {
          "url": "webpack://kts-2023-github-client/src/store",
          "path": "${workspaceFolder}/src/store"
        },
        {
          "url": "webpack://kts-2023-github-client/src/pages",
          "path": "${workspaceFolder}/src/pages"
        }
      ]
    }
  ]
}